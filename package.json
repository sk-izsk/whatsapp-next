  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier:check": "prettier --config .prettierrc --check \"*/**/*.{ts,tsx,js,jsx,json,sass,scss,css,html,md}\"",
